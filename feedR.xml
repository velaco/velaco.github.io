---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <atom:link href="https://velaco.github.io/feedR.xml" rel="self" type="application/rss+xml" />
    <title>Curiosity Killed the Cat â€“ (... but satisfaction brought it back.)</title>
    <description>R posts from the blog Curiosity Killed the Cat</description>
    <author>
      <name>Aleksandar Ratesic</name>
    </author>
    <link>{{ site.url }}</link>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts %}
		{% unless post.draft %}
			{% for tag in post.tags %}	
				{% if tag == 'R' %}
				  <item>
					  <title>{{ post.title | xml_escape }}</title>
					  <description>{{ post.content | xml_escape }}</description>
					  <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
					  <link>{{ post.url | prepend: site.url }}</link>
					  <guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
					  {% for tag in post.tags %}
					      <category>{{ tag | xml_escape }}</category>
					  {% endfor %}
				  </item>
				{% endif %}
			{% endfor %}
		 {% endunless %}
    {% endfor %}
  </channel>
</rss>
